<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CActorModel id="RedeemerAttackBeamImpactReverse">
        <Supporter Subject="::Creator"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="SupporterDestruction" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <Host Subject="RedeemerAttackImpactSiteReverse"/>
        <Model value="ColossusAttackBeamImpact"/>
    </CActorModel>
    <CActorModel id="RedeemerAttackBeamImpact">
        <Supporter Subject="::Creator"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="ActorCreation" Send="Create ColossusAttackImpactSound"/>
        <On Terms="SupporterDestruction" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <Host Subject="RedeemerAttackImpactSite"/>
        <Model value="ColossusAttackBeamImpact"/>
    </CActorModel>
    <CActorSiteMover id="RedeemerAttackImpactSite">
        <Terms value="Cap 1"/>
        <Sharing value="PerEffectTree"/>
        <On Terms="Effect.NodRedeemerThermalLancesEA.Impact" Send="Create"/>
        <On Terms="Effect.NodRedeemerThermalLancesEA.Impact; At Effect" Send="MoverSetDestination2D"/>
        <On Terms="MoverArrived" Target="NodRedeemerAttackBeam" Send="Signal Arrival"/>
        <!--Extra Weapons-->
        <On Terms="Effect.NodRedeemerThermalLancesEA2.Impact" Send="Create"/>
        <On Terms="Effect.NodRedeemerThermalLancesEA2.Impact; At Effect" Send="MoverSetDestination2D"/>
        <On Terms="MoverArrived" Target="NodRedeemerAttackBeam2" Send="Signal Arrival"/>
        <HostSiteOps Ops="SOpShadow"/>
        <Acceleration value="40.000000"/>
        <Deceleration value="40.000000"/>
        <Speed value="2.000000"/>
        <SpeedMax value="4.100000"/>
    </CActorSiteMover>
    <CActorSiteMover id="RedeemerAttackImpactSiteReverse">
        <Terms value="Cap 1"/>
        <Sharing value="PerEffectTree"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse.Impact" Send="Create"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse.Impact; At Effect" Send="MoverSetDestination2D"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverseExtra" Send="Signal Arrival"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverse" Send="Signal Arrival"/>
        <!--Extra Weapons-->
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse2.Impact" Send="Create"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse2.Impact; At Effect" Send="MoverSetDestination2D"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverseExtra2" Send="Signal Arrival"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverse2" Send="Signal Arrival"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse3.Impact" Send="Create"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse3.Impact; At Effect" Send="MoverSetDestination2D"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverseExtra3" Send="Signal Arrival"/>
        <On Terms="MoverArrived" Target="RedeemerAttackBeamReverse3" Send="Signal Arrival"/>
        <HostSiteOps Ops="SOpShadow"/>
        <Acceleration value="40.000000"/>
        <Deceleration value="40.000000"/>
        <Speed value="2.000000"/>
        <SpeedMax value="4.100000"/>
    </CActorSiteMover>
    <CActorSound id="RedeemerAttackLaunchSound" parent="SoundOneShot">
        <Host Subject="Colossus"/>
        <Sound value="Redeemer_AttackLaunch"/>
    </CActorSound>
    <CActorSite id="RedeemerAttackLaunchSite" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachWeapon00"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSite2" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachWeapon01"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSite3" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachWeapon02"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteR1" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint01 SOpHigher005"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteR2" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint01 SOpHigher005"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteR3" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint01 SOpHigher005"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteL1" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint02 SOpHigher005"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteL2" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint02 SOpHigher005"/>
    </CActorSite>
    <CActorSite id="RedeemerAttackLaunchSiteL3" parent="SiteUNIT">
        <Aliases value="_LaunchGuide"/>
        <HostSiteOps Ops="SOpAttachHardPoint02 SOpHigher005"/>
    </CActorSite>


    <CActorAction id="RedeemerAttack" parent="GenericAttack" effectAttack="RedeemerBeamDamage">
        <LaunchAttachQuery Methods="Weapon 0"/>
        <LaunchAssets Sound="Redeemer_AttackLaunch"/>
        <ImpactPhysics Name="RailgunImpact" MatchKeys="Basic" Physics="CNCRailgunAttackDeathForce"/>
        <ImpactMap index="None" Model="RedeemerAttackBeamImpact"/>
    </CActorAction>
    <CActorAction id="RedeemerAttack2" parent="GenericAttack" effectAttack="RedeemerBeamDamage2">
        <LaunchAttachQuery Methods="Weapon 1"/>
        <LaunchAssets Sound="Redeemer_AttackLaunch"/>
        <Beam value="RedeemerAttackBeam2"/>
        <ImpactPhysics Name="RailgunImpact" MatchKeys="Basic" Physics="CNCRailgunAttackDeathForce"/>
        <ImpactMap index="None" Model="RedeemerAttackBeamImpact" Sound="RedeemerAttackImpact" ModelReaction="RedeemerAttackImpactReaction"/>
        <ImpactMap index="Shield" Sound="RedeemerAttackImpactShield" ModelReaction="RedeemerAttackImpactReactionShield"/>
    </CActorAction>
    <CActorAction id="RedeemerAttack3" parent="GenericAttack" effectAttack="RedeemerBeamDamage3">
        <LaunchAttachQuery Methods="Weapon 2"/>
        <LaunchAssets Sound="Redeemer_AttackLaunch"/>
        <Beam value="RedeemerAttackBeam3"/>
        <ImpactPhysics Name="RailgunImpact" MatchKeys="Basic" Physics="CNCRailgunAttackDeathForce"/>
        <ImpactMap index="None" Model="RedeemerAttackBeamImpact" Sound="RedeemerAttackImpact" ModelReaction="RedeemerAttackImpactReaction"/>
        <ImpactMap index="Shield" Sound="RedeemerAttackImpactShield" ModelReaction="RedeemerAttackImpactReactionShield"/>
    </CActorAction>


    <CActorBeamSimple id="RedeemerAttackBeam" parent="GenericAttackBeam">
        <On Terms="ActorOrphan" Send="Destroy"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeam2" parent="GenericAttackBeam">
        <On Terms="ActorOrphan" Send="Destroy"/>
        <Model value="RedeemerAttackBeam"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeam3" parent="GenericAttackBeam">
        <On Terms="ActorOrphan" Send="Destroy"/>
        <Model value="RedeemerAttackBeam"/>
    </CActorBeamSimple>



    <CActorBeamSimple id="NodRedeemerAttackBeam">
        <Terms value="Cap 1"/>
        <On Terms="Effect.NodRedeemerThermalLancesEA.Impact" Send="Create"/>
        <On Terms="ActorCreation" Send="Create RedeemerAttackBeamImpact"/>
        <On Terms="ActorCreation" Send="Create RedeemerAttackLaunchSound"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="Signal.*.Arrival" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterOpening; AnimName BSD" Target="RedeemerAttackImpactSite" Send="MoverMove"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <Model value="RedeemerAttackBeam2"/>
        <HostLaunch Subject="RedeemerAttackLaunchSite" Scope="Caster" Actor="Create"/>
        <HostImpact Subject="RedeemerAttackImpactSite"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverse">
        <Terms value="Cap 1"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse.Impact" Send="Create"/>
        <On Terms="ActorCreation" Send="Create RedeemerAttackBeamImpactReverse"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="Signal.*.Arrival" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterOpening; AnimName BSD" Target="RedeemerAttackImpactSiteReverse" Send="MoverMove"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <Model value="RedeemerAttackBeam2"/>
        <HostLaunch Subject="RedeemerAttackLaunchSite2" Scope="Caster" Actor="Create"/>
        <HostImpact Subject="RedeemerAttackImpactSiteReverse"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverseExtra">
        <Terms value="Cap 1"/>
        <On Terms="Effect.NodRedeemerThermalLancesEAReverse.Impact" Send="Create"/>
        <On Terms="ActorCreation" Send="Create RedeemerAttackBeamImpactReverse"/>
        <On Terms="ActorCreation" Send="AnimBracketStart BSD Birth Stand Death"/>
        <On Terms="Signal.*.Arrival" Send="AnimBracketStop BSD"/>
        <On Terms="AnimBracketState.*.AfterOpening; AnimName BSD" Target="RedeemerAttackImpactSiteReverse" Send="MoverMove"/>
        <On Terms="AnimBracketState.*.AfterClosing; AnimName BSD" Send="Destroy"/>
        <Model value="RedeemerAttackBeam2"/>
        <HostLaunch Subject="RedeemerAttackLaunchSite3" Scope="Caster" Actor="Create"/>
        <HostImpact Subject="RedeemerAttackImpactSiteReverse"/>
    </CActorBeamSimple>

    <CActorBeamSimple id="NodRedeemerAttackBeam2" parent="RedeemerAttackBeam">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEA2.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteR1" Scope="Caster" Actor="Create"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverse2" parent="RedeemerAttackBeamReverse">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEAReverse2.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteR2"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverseExtra2" parent="RedeemerAttackBeamReverseExtra">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEAReverse2.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteR3"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="NodRedeemerAttackBeam3" parent="RedeemerAttackBeam">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEA3.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteL1" Scope="Caster" Actor="Create"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverse3" parent="RedeemerAttackBeamReverse">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEAReverse3.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteL2"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="RedeemerAttackBeamReverseExtra3" parent="RedeemerAttackBeamReverseExtra">
        <On index="0" Terms="Effect.NodRedeemerThermalLancesEAReverse3.Impact" Send="Create"/>
        <HostLaunch Subject="RedeemerAttackLaunchSiteL3"/>
    </CActorBeamSimple>






</Catalog>