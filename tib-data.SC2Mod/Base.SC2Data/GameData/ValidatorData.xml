<?xml version="1.0" encoding="utf-8"?>
<Catalog>
<!--    workers-->
    <CValidatorCombine id="isCNCWorker">
        <CombineArray value="IsDozer"/>
        <CombineArray value="IsSlave"/>
        <CombineArray value="IsNodHarvester"/>
        <CombineArray value="IsScrinHarvester"/>
        <CombineArray value="IsGDIHarvester"/>
    </CValidatorCombine>

    <CValidatorCombine id="TimewarpProduction">
        <CombineArray value="NodConstructionYard"/>
        <CombineArray value="NodCrane"/>
        <CombineArray value="NodTiberiumRefinery"/>
        <CombineArray value="NodHand"/>
        <CombineArray value="NodWarFactory"/>
        <CombineArray value="NodRedeemerFacility"/>
        <CombineArray value="NodAirTower"/>
    </CValidatorCombine>
    <CValidatorCombine id="TimewarpResearch">
        <CombineArray value="NodChemicalPlant"/>
        <CombineArray value="NodShrine"/>
        <CombineArray value="NodTechLab"/>
        <CombineArray value="NodOperationsCenter"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsPowerOrSupplySource">
        <CombineArray value="IsPowerSource"/>
        <CombineArray value="IsSupplySource"/>
    </CValidatorCombine>
    <CValidatorCombine id="IsPowerSource">
        <CombineArray value="HasCnCPowerGeneration"/>
        <CombineArray value="HasCnCPowerGeneration005"/>
        <CombineArray value="HasCnCPowerGeneration015"/>
        <CombineArray value="HasCnCPowerGeneration030"/>
        <CombineArray value="HasCNCConstructionRadius"/>
        <CombineArray value="HasCNCConstructionRadiusSmall"/>
        <CombineArray value="HasCNCConstructionRadiusMedium"/>
    </CValidatorCombine>
    <CValidatorUnitCompareBehaviorCount id="HasCnCPowerGeneration">
        <Behavior value="CnCPowerGeneration"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCnCPowerGeneration005">
        <Behavior value="CnCPowerGeneration005"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCnCPowerGeneration015">
        <Behavior value="CnCPowerGeneration015"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCnCPowerGeneration030">
        <Behavior value="CnCPowerGeneration030"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCNCConstructionRadius">
        <Behavior value="CNCConstructionRadius"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCNCConstructionRadiusSmall">
        <Behavior value="CNCConstructionRadiusSmall"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorUnitCompareBehaviorCount id="HasCNCConstructionRadiusMedium">
        <Behavior value="CNCConstructionRadiusMedium"/>
    </CValidatorUnitCompareBehaviorCount>
    <CValidatorCombine id="IsSupplySource">
        <Type value="And"/>
        <CombineArray value="IsSupplySourceBase"/>
        <CombineArray value="NotResourceDropOffMinerals"/>
        <CombineArray value="NotResourceDropOffVespene"/>
    </CValidatorCombine>
    <CValidatorUnitCompareField id="IsSupplySourceBase">
        <Field value="Food"/>
        <Value value="0"/>
        <Compare value="GT"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="NotResourceDropOffMinerals">
        <Field value="ResourceDropOff[Minerals]"/>
        <Value value="0"/>
    </CValidatorUnitCompareField>
    <CValidatorUnitCompareField id="NotResourceDropOffVespene">
        <Field value="ResourceDropOff[Vespene]"/>
        <Value value="0"/>
    </CValidatorUnitCompareField>
</Catalog>
