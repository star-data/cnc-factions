    <CEffectLaunchMissile id="DominationLM">
        <EditorCategories value="Race:Terran"/>
        <LaunchLocation Value="CasterUnit"/>
        <AmmoUnit value="DominationWeapon"/>
        <ImpactEffect value="DominationDummy"/>
        <ValidatorArray index="0" removed="1"/>
