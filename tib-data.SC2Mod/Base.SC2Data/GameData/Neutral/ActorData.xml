<Catalog>

    <CActorUnit id="Visceroid" wireframe="Wireframe-visceroid" unitName="Visceroid" icon="btn-visceroid">
        <On Terms="UnitBirth.*.Normal" Send="Create CorpserBirth"/>
        <On Terms="UnitHeightUpdate.*.Land" Send="Create Broodling_Land"/>
        <DeathArray index="Normal" SoundLink="Broodling_Explode"/>
        <DeathArray index="Disintegrate" AnimProps="Death" SoundLink="Broodling_DeathFXAcid"/>
        <DeathArray index="Fire" SoundLink="Broodling_DeathFXFire"/>
        <PortraitModel value="SymbiotePortrait"/>
        <WalkAnimMoveSpeed value="1.5"/>
        <BarOffset value="25"/>
        <BarWidth value="40"/>
        <LifeArmorIcon value="Assets\textures\btn-upgrade-zerg-groundcarapace-level0.dds"/>
        <ShieldArmorIcon value="Assets\icons\btn-upgrade-zerg-swarmfrenzy.dds"/>
        <StatusColors index="Shields">
            <ColorArray index="0" value="255,0,128,0"/>
        </StatusColors>
        <VitalColors index="Shields">
            <ColorArray value="255,0,128,0"/>
        </VitalColors>
        <VitalNames index="Shields" value="Vital/ZergShields"/>
        <WireframeShieldColor value="255,0,128,0"/>
    </CActorUnit>
    <CActorUnit id="TibMineralField" parent="MineralBase" unitName="TibMineralField">
        <On Terms="ActorCreation; Option Shader Ultra" Send="ModelSwap MineralFieldHigh"/>
        <On Terms="ActorCreation; Option Shader Extreme" Send="ModelSwap MineralFieldHigh"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 0" Send="AnimGroupApply A"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 0" Send="AnimGroupRemove A"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 1" Send="AnimGroupApply B"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 1" Send="AnimGroupRemove B"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 2" Send="AnimGroupApply C"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 2" Send="AnimGroupRemove C"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 3" Send="AnimGroupApply D"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 3" Send="AnimGroupRemove D"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 0" Send="Create MineralFieldSplatSmall"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 0" Target="MineralFieldSplatSmall" Send="Destroy"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 1" Send="Create MineralFieldSplatSmall"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 1" Target="MineralFieldSplatSmall" Send="Destroy"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 2" Send="Create MineralFieldSplat"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 2" Target="MineralFieldSplat" Send="Destroy"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Start; Level 3" Send="Create MineralFieldSplat"/>
        <On Terms="BehaviorLevel.TiberiumMineralField40.Stop; Level 3" Target="MineralFieldSplat" Send="Destroy"/>
        <On Terms="ActorCreation" Send="SetTintColor {0,255,0 2.500000}"/>
        <On Terms="UnitBirth.TibMineralField" Send="Create LightOmniGreen"/>
        <Model value="MineralField"/>
        <BuildModel value="MineralField"/>
        <DeathArray index="Normal" ModelLink="MineralField"/>
        <PlacementModel value="MineralField"/>
        <GroupIcon>
            <Image value="Assets\textures\Wireframe-General-MineralField.dds"/>
        </GroupIcon>
        <HeroIcon value="Assets\textures\Wireframe-General-MineralField.dds"/>
        <StatusBarFlags index="Shields" value="0"/>
        <StatusBarFlags index="Life" value="0"/>
        <StatusBarFlags index="Energy" value="0"/>
        <StatusBarFlags index="Cargo" value="0"/>
        <StatusBarFlags index="Progress" value="0"/>
        <StatusBarFlags index="Duration" value="0"/>
        <StatusBarFlags index="Magazine" value="0"/>
        <UnitFlags index="ShowBlobShadow" value="0"/>
        <UnitIcon value="Assets\textures\Wireframe-General-MineralField.dds"/>
        <Wireframe>
            <Image value="Assets\textures\Wireframe-General-MineralField.dds"/>
        </Wireframe>
    </CActorUnit>
</Catalog>