    <CActorSound id="CNCElectricSparksSounds" parent="SoundOneShot">
        <On Terms="Effect.NodEMPAttackSearch.Start" Send="Create"/>
        <On Terms="Effect.NodVisionSearchArea.Start" Send="Create"/>
    </CActorSound>
    <CActorSiteOpBanker id="CnCBankerAir" parent="SOpBanker">
        <RollAngleMax value="75.000000"/>
        <RollInRate value="23.450001"/>
        <RollOutRate value="18.799999"/>
    </CActorSiteOpBanker>
    <CActorUnit id="DockDummy" icon="AirTower" parent="GenericUnitStandard" wireframe="NodAirTowerWireframe" unitName="DockDummy">
        <Model value="Invisible"/>
    </CActorUnit>
    <CActorSiteOpLocalOffset id="SOpHigher005">
        <LocalOffset value="0.000000,0.000000,0.050000"/>
    </CActorSiteOpLocalOffset>
    <CActorSiteOpForward id="SOpForwardWeapon1">
        <Forward value="0.000000,-1.500000,0.000000"/>
        <HostForward Subject="_Unit" Scope="Caster"/>
        <HostForwardSiteOps Ops="SOpAttachWeapon"/>
    </CActorSiteOpForward>
    <CActorEventMacro id="PhysicsDeathsVoidGround">
        <On Terms="UnitDeathCustomize; KilledByEffect NodMilitantRocketWeaponDamage" Send="DeathCustomize RagdollSiegeTank"/>
        <On Terms="UnitDeathCustomize; KilledByEffect NodMilitantRocketWeaponDamageUp" Send="DeathCustomize RagdollSiegeTank"/>
    </CActorEventMacro>
    <CActorSound id="CYMorphSound" parent="SoundOneShot">
        <On Terms="AbilMorph.*.Start; MorphTo ZOCOMDefenseCrawler" Send="Create"/>
        <On Terms="UnitDeath.ZOCOMDefenseCrawler" Send="Destroy"/>
        <On Terms="AbilMorph.*.Start; MorphTo CABALDefenseCrawler" Send="Create"/>
        <Sound value="Crawler_Morph"/>
    </CActorSound>
    <CActorSound id="MCVMorphSound" parent="SoundOneShot">
        <On Terms="AbilMorph.*.Start; MorphTo ZOCOMDefenseCrawlerUnpack" Send="Create"/>
        <On Terms="UnitDeath.ZOCOMDefenseCrawlerUnpack" Send="Destroy"/>
        <On Terms="AbilMorph.*.Start; MorphTo CABALDefenseCrawlerDeployed" Send="Create"/>
        <Sound value="Crawler_Morph"/>
    </CActorSound>
    <CActorSound id="Ghost_CloakOn" parent="SoundOneShot">
        <Terms value="ValidateUnit NotCloaked"/>
        <On Terms="Behavior.GhostCloak.On" Send="Create"/>
        <On Terms="Behavior.NodHarvesterCloak.On" Send="Create"/>
    </CActorSound>
    <CActorSound id="Ghost_CloakOff" parent="SoundOneShot">
        <Terms value="ValidateUnit NotCloakedAndNotDead"/>
        <On Terms="Behavior.GhostCloak.Off" Send="Create"/>
        <On Terms="Behavior.NodHarvesterCloak.Off" Send="Create"/>
    </CActorSound>
    <CActorEventMacro id="CNCUnpoweredAnim">
        <On Terms="Behavior.NodEnergyProduction.On" Send="SetTintColor 150,150,200 1.000000"/>
        <On Terms="Behavior.NodEnergyProduction.On" Send="SetTintColor 150,150,200 "/>
        <On Terms="Behavior.NodEnergyProduction.Off" Send="SetTintColor 255,255,255 1.000000"/>
        <On Terms="Behavior.NodEnergyProduction.Off" Send="SetTintColor 255,255,255"/>
        <On Terms="Behavior.NodEnergyProductionDefense.On" Send="SetTintColor 75,75,125 1.000000"/>
        <On Terms="Behavior.NodEnergyProductionDefense.On" Send="SetTintColor 75,75,125 "/>
        <On Terms="Behavior.NodEnergyProductionDefense.Off" Send="SetTintColor 255,255,255 1.000000"/>
        <On Terms="Behavior.NodEnergyProductionDefense.Off" Send="SetTintColor 255,255,255"/>

        <!--        <On Terms="Behavior.NodProductionBoost.On" Send="SetTintColor 150,150,150 1.000000"/>-->
